{
  "name": "vito",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "cd dashboard && rm -rf dist && npm run build && cd .. && pm2 start user/ecosystem.config.cjs",
    "stop": "pm2 stop vito-server",
    "restart": "pm2 restart vito-server",
    "logs": "pm2 logs vito-server",
    "status": "pm2 status",
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "build:dashboard": "cd dashboard && npm run build",
    "dev:dashboard": "cd dashboard && npm run dev"
  },
  "dependencies": {
    "@mariozechner/pi-ai": "latest",
    "@mariozechner/pi-coding-agent": "latest",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.13.5",
    "better-sqlite3": "^11.0.0",
    "cors": "^2.8.6",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "execa": "^9.6.1",
    "express": "^5.2.1",
    "googleapis": "^171.4.0",
    "grammy": "^1.40.0",
    "gray-matter": "^4.0.3",
    "node-cron": "^4.2.1",
    "openai": "^4.104.0",
    "playwright": "^1.58.2",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.18.1",
    "tsx": "^4.0.0",
    "typescript": "^5.9.3"
  }
}
