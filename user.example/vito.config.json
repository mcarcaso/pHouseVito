{
  "model": {
    "provider": "anthropic",
    "name": "claude-sonnet-4-5"
  },
  "memory": {
    "currentSessionLimit": 50,
    "crossSessionLimit": 30,
    "memoriesLimit": 10,
    "compactionThreshold": 200
  },
  "embeddings": {
    "provider": "openai",
    "model": "text-embedding-3-small"
  },
  "channels": {
    "dashboard": {
      "enabled": true,
      "streamMode": "stream"
    },
    "telegram": {
      "enabled": false,
      "streamMode": "bundled",
      "allowedChatIds": []
    }
  },
  "cron": {
    "jobs": []
  }
}
